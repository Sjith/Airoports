<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Base application theme is the default theme. -->


    <!-- Variation on the Light theme that turns off the title -->
    <style name="Theme.Airoports" parent="android:style/Theme.Light">
        <item name="android:windowNoTitle">true</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="tab_background">
        <item name="android:background">@drawable/tab_button</item>
    </style>

    <style name="button_background_survey_list">
        <item name="android:background">#01894B</item>
        <item name="android:height">40dip</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>

</resources>